		TGMobileShare({
			shareTitle: '王者荣耀攻略站-李白英雄攻略',
			shareImgUrl: 'https://game.gtimg.cn/images/yxzj/img201606/skin/hero-info/131/131-bigskin-1.jpg',
			actName: 'zsbdm-hero131'
		});

		// 技能加点切换
		setArrow();

		function setArrow() {
			var $list = $('.plus-tab'),
				counts = $list.children().length,
				$control = $('.plus-control'),
				oddWidth = $('#PlusTab').width(),
				_w = Math.ceil(counts / 5),
				$prev = $('.plus-prev'),
				$next = $('.plus-next'),
				i = 0;

			if (counts > 5) {
				$control.css('display', 'block');
				$list.css('width', _w + '00%');

				init();
			}

			function init() {
				$prev.on('click', function () {
					if (i == 0) return false;
					i--;
					var px = -i * oddWidth + 'px';
					$list.css({
						'transform': 'translate3d(' + px + ', 0, 0)'
					});
					arrowShow();
				});

				$next.on('click', function () {
					i++;
					var px = -i * oddWidth + 'px';
					if (i + 1 == _w) {
						$(this).hide();
					}
					$list.css({
						'transform': 'translate3d(' + px + ', 0, 0)'
					});
					arrowShow();
				});
			}

			function arrowShow() {
				if (i == 0) {
					$prev.css('display', 'none');
				} else {
					$prev.css('display', 'block');
				}

				if (i + 1 == _w) {
					$next.css('display', 'none');
				} else {
					$next.css('display', 'block');
				}
			}
		}
